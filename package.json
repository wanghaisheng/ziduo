    "chokidar": "^3.6.0",
    "cross-env": "^7.0.3",
    "eslint-import-resolver-typescript": "^3.6.1",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.38",
    "postcss-less": "^6.0.0",
    "postcss-pxtorpx-pro": "^2.0.0",
    "rollup": "^4.18.0",
    "stylelint": "^16.6.1",
    "stylelint-config-standard": "^36.0.0",
    "terser": "^5.31.1",
    "typescript": "^5.4.5",
    "xo": "^0.58.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint",
        "eslint-plugin-import",
        "webpack"
